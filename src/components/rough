import React from 'react';
import "../css/post.css";

class Post extends React.Component {
  constructor(props) {
    super(props)
  
    this.state = {
      show : true,
      
    }
  }
  
  categoryTitles = [
    "Mobiles",
    "Vehicles",
    "Property for Sale",
    "Property for Rent",
    "Electronics & Home Appliances",
    "Bikes",
    "Business,Industrial & Agriculture",
    "Services",
    "Jobs",
    "Animals",
    "Furniture & Home Decor",
    "Fashion & Beauty",
    "Books,Sports & Hobbies",
    "Kids"
  ];
  categoryIcons = [
    "fa fa-mobile mr-3 ml-2 caticons",
    "fa fa-cab mr-3 caticons",
    "fa fa-home mr-3 caticons",
    "fa fa-home mr-3 caticons",
    "fa fa-laptop mr-3 caticons",
    "fa fa-motorcycle mr-3 caticons",
    "fa fa-industry mr-3 caticons",
    "fa fa-cutlery mr-3 caticons",
    "fa fa-suitcase mr-3 caticons",
    "fa fa-paw mr-3 caticons",
    "fa fa-bed mr-3 caticons",
    "fa fa-female mr-3 caticons",
    "fa fa-book mr-3 caticons",
    "fa fa-user-secret mr-3 caticons"
  ];

  subCategory = [
    "Tablets",
    "Accessories",
    "Mobile Phones"
  ];

  dispSub = (e) => {
     var cat = e.childNodes[1].data;
     var subul = document.createElement("ul");
     var outerdiv = document.getElementsByClassName("outer")[0];
   
     switch(cat){
     case "Mobiles":
        this.setState({
          show:true
        })
            this.subCategory.map((v,i)=>{
              var subli = document.createElement("li");
              subul.className = this.state.show ? "MobileCat list-unstyled d-block" : "subcat list-unstyled d-none";
              subli.innerHTML = this.subCategory[i];
              subli.className = "item" + i + " "+ "list-group-item-action w-50 border ml-2 p-3";
              subul.style.top = "30%";
              subul.appendChild(subli);
            })
            this.setState({
              show:false
            })
            break;
      case "Vehicles":
        this.setState({
          show:true
        })
        this.subCategory.map((v,i)=>{
          var subli = document.createElement("li");
          subul.className = this.state.show ? "VehicleCat list-unstyled d-block" : "subcat list-unstyled d-none";
          alert(this.state.show)
          subli.innerHTML = this.subCategory[i];
          subli.className = "item" + i + " "+ "list-group-item-action w-50 border ml-2 p-3";
          subul.style.top = "30%";
          subul.appendChild(subli);
        })
        this.setState({
          show:false
        })
        break;
      default:
        alert("defa")
        break;

       }
       outerdiv.appendChild(subul);   
     }

  render() {
    return (
     <>
      <strong className="text-center d-block"><h4 className="font-weight-bold">POST YOUR AD</h4></strong>
      <div className="outer border mb-2">
        <strong className="d-flex row m-1 p-3">CHOOSE A CATEGORY</strong>
        <ul className="ul list-unstyled" id="ul">
          {this.categoryTitles.map((v, i) => (
            <li key={i} onClick={(e)=>this.dispSub(e.target)} className={"Cat" + i + " " + "list-group-item-action w-50 border ml-2 p-3"}><i className={this.categoryIcons[i]}></i>{v}<i className="fa fa-angle-right float-right forwicon"></i></li>
          ))}


        </ul>
       </div>
     </>
      )
  }
}

export default Post;
